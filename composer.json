{
    "name": "alien/yii2-sweet-alert",
    "description": "Asset for use SweetaAlert2 and replase standart js dialogs.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","sweetalert","asset"],
    "license": "BSD-3-Clause",
    "version": "0.1",
    "authors": [
        {
            "name": "Alien-art",
            "email": "alien@alien-art.ru"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sweetalert2/sweetalert2/tree/dist"
        },
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/sweetalert2": "@dev"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {
            "alien\\sweetalert\\": ""
        }
    }
}
