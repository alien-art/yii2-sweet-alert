{
    "name": "alien/yii2-sweet-alert",
    "description": "Asset for use SweetaAlert2 and replase standart js dialogs.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","sweetalert","asset"],
    "license": "BSD-3-Clause",
    "version": "0.1",
    "authors": [
        {
            "name": "Alien-art",
            "email": "alien@alien-art.ru"
        }
    ],
    "minimum-stability": "@stable",
    "require": {
        "yiisoft/yii2": "*",
        "npm-asset/sweetalert2": "*"
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm"
        }
    },
    "autoload": {
        "psr-4": {
            "alien\\sweetalert\\": ""
        }
    }
}
